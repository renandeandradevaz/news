<%= stylesheet_link_tag  "salvarnoticia" %>

<%= form_for(@noticia,  html: {class: "form_salvar_noticia"}) do |f| %>
    <%= f.label :titulo %><br>
    <%= f.text_field :titulo %>
    <br> <br> <br>
    <%= f.label :corpo %><br>
    <%= f.text_area(:corpo) %>
    <br> <br> <br>
    <%= f.submit 'Salvar' %>
<% end %>

